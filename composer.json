{
    "name": "shippinno/notification",
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "adbario/php-dot-notation": "^2.1",
        "alek13/slack": "^2.2",
        "doctrine/orm": "^2.5",
        "mathiasverraes/classfunctions": "^1.1",
        "shippinno/email": "^1.0.0",
        "shippinno/template": "^1.0",
        "tanigami/doctrine-json-unescaped-type": "^1.0",
        "tanigami/specification": "^1.3",
        "tanigami/value-objects": "^0.4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.2",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shippinno\\Notification\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
