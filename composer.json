{
    "name": "shippinno/notification",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/doctrine-json-unescaped-type.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/email.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/template-php.git",
            "no-api": true
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "adbario/php-dot-notation": "^2.1",
        "alek13/slack": "^2.2",
        "doctrine/orm": "^2.5",
        "mathiasverraes/classfunctions": "^1.1",
        "shippinno/email": "dev-feature/php-version-up",
        "shippinno/template": "dev-feature/php-version-up",
        "tanigami/doctrine-json-unescaped-type": "dev-php-version-up",
        "tanigami/specification": "^1.3",
        "tanigami/value-objects": "^0.4.6",
        "symfony/cache": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shippinno\\Notification\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
