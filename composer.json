{
    "name": "shippinno/notification",
    "type": "library",
    "repositories": [
        {
          "type": "vcs",
          "url": "git@github.com:shippinno/email.git",
          "no-api": true
        }
      ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "adbario/php-dot-notation": "^3.0.0",
        "alek13/slack": "^2.2",
        "doctrine/orm": "^2.16",
        "mathiasverraes/classfunctions": "^1.1",
        "shippinno/email": "dev-feature/php-version-up",
        "shippinno/template": "^1.0",
        "tanigami/doctrine-json-unescaped-type": "^1.0",
        "tanigami/specification": "^1.3",
        "tanigami/value-objects": "^0.4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shippinno\\Notification\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
